{
  "name": "react-google-maps_examples_simple-map",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./public/index.html ./public/assets",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production reacthtmlpack buildToDir ./public './src/views/*.html.js'",
    "predev": "npm run clean",
    "dev": "npm run clean && reacthtmlpack buildToDir ./public './src/views/*.html.js'",
    "prestart": "npm run clean",
    "start": "reacthtmlpack devServer ./Client.webpackConfig.js ./public './src/views/*.html.js'"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "reacthtmlpack": "^1.2.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "classnames": "^2.1.5",
    "node-libs-browser": "^0.5.3",
    "react": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-google-maps": "^4.2.0"
  }
}
